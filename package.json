{
  "name": "impact",
  "version": "1.0.0",
  "description": "a port of: 'The awesomest way to create even more awesome HTML5 games!'",
  "scripts": {
    "clean": "rm -rf .cache/ coverage/ dist/",
    "reset": "yarn clean node_modules/ && yarn install",
    "start": "parcel src/index.html & serve exclude/jumpnrun && fg",
    "build": "parcel build src/index.html --public-url /Impact",
    "build:tornaire": "parcel build src/games/tornaire/index.html --public-url /Impact --no-source-maps",
    "deploy": "gh-pages -d dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysterycommand/Impact.git"
  },
  "keywords": [
    "javascript",
    "game",
    "engine",
    "html5",
    "desktop",
    "mobile"
  ],
  "author": "Dominic Szablewski <dominic.szablewski@gmail.com>",
  "contributors": [
    "Matt Hayes <matt@mysterycommand.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysterycommand/Impact/issues"
  },
  "homepage": "https://github.com/mysterycommand/Impact#readme",
  "engines": {
    "node": "12.14.1",
    "yarn": "1.21.1"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "core-js": "3.6.4",
    "gh-pages": "2.2.0",
    "parcel-bundler": "1.12.4",
    "prettier": "1.19.1",
    "serve": "11.3.0",
    "typescript": "3.7.5"
  }
}
